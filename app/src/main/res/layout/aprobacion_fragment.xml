<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent" android:id="@+id/constraintLayout">

    <TextView
            android:text="aprobacion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="76dp"
            android:id="@+id/barra"
            android:background="@color/colorPrimaryDark"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0">

        <androidx.appcompat.widget.AppCompatImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/basicos_btnIngresos"
                app:layout_constraintStart_toEndOf="@+id/basicos_btnBasicos"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintEnd_toStartOf="@+id/basicos_btnAprobacion" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toTopOf="@+id/textView6"/>
        <androidx.appcompat.widget.AppCompatImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/basicos_btnBasicos"
                app:layout_constraintStart_toEndOf="@+id/view"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintEnd_toStartOf="@+id/basicos_btnIngresos" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toTopOf="@+id/textView4"
                android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
        <androidx.appcompat.widget.AppCompatImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/basicos_btnAprobacion"
                app:layout_constraintStart_toEndOf="@+id/basicos_btnIngresos"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/view2"
                android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/textView5" android:layout_marginRight="16dp"
                android:layout_marginEnd="16dp"/>
        <androidx.appcompat.widget.AppCompatImageButton
                android:text="Button"
                android:layout_width="43dp"
                android:layout_height="45dp"
                android:src="@drawable/ic_backrigth"
                android:background="@android:color/transparent"
                android:id="@+id/aprobacionContinuar" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="40dp"
                android:layout_marginRight="40dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView3"
                app:layout_constraintStart_toEndOf="@+id/textView3"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintTop_toTopOf="@+id/textView3"
        />
        <androidx.appcompat.widget.AppCompatImageButton
                android:text="Button"
                android:layout_width="43dp"
                android:layout_height="45dp"
                android:src="@drawable/ic_back"
                android:background="@android:color/transparent"
                android:id="@+id/aprobacionRegresar"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.7"
                app:layout_constraintEnd_toStartOf="@+id/textView2"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintBottom_toBottomOf="@+id/textView2"
                app:layout_constraintTop_toTopOf="@+id/textView2"/>
        <androidx.appcompat.widget.AppCompatTextView
                android:text="regresar"
                android:textColor="@color/colorWhite"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView2" app:layout_constraintStart_toEndOf="@+id/ingresarRegresar"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/view"
                android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
        <androidx.appcompat.widget.AppCompatTextView
                android:text="continuar"
                android:textColor="@color/colorWhite"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView3" app:layout_constraintStart_toEndOf="@+id/view2"
                app:layout_constraintHorizontal_bias="0.3" app:layout_constraintEnd_toStartOf="@+id/ingresosContinuar"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

        <ImageView android:layout_width="1dp"
                   android:background="@color/colorWhite"
                   android:layout_height="match_parent"
                   android:id="@+id/view2"
                   app:layout_constraintTop_toTopOf="parent"
                   app:layout_constraintBottom_toBottomOf="parent"
                   android:layout_marginTop="16dp"
                   android:layout_marginBottom="16dp" app:layout_constraintEnd_toEndOf="parent"
                   android:layout_marginEnd="180dp" android:layout_marginRight="180dp"
                   app:layout_constraintVertical_bias="1.0"/>

        <ImageView android:layout_width="1dp"
                   android:background="@color/colorWhite"
                   android:layout_height="0dp"
                   android:id="@+id/view"
                   app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"
                   app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
                   app:layout_constraintVertical_bias="0.0" app:layout_constraintStart_toStartOf="parent"
                   android:layout_marginLeft="200dp" android:layout_marginStart="200dp"/>

        <androidx.appcompat.widget.AppCompatTextView
                android:text="basicos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView4"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/basicos_btnBasicos"
                app:layout_constraintEnd_toEndOf="@+id/basicos_btnBasicos"
                app:layout_constraintStart_toStartOf="@+id/basicos_btnBasicos"/>
        <androidx.appcompat.widget.AppCompatTextView
                android:text="aprobacion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/basicos_btnAprobacion"
                app:layout_constraintStart_toStartOf="@+id/basicos_btnAprobacion"
                app:layout_constraintEnd_toEndOf="@+id/basicos_btnAprobacion"/>
        <androidx.appcompat.widget.AppCompatTextView
                android:text="ingresos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView6"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/basicos_btnIngresos"
                app:layout_constraintStart_toStartOf="@+id/basicos_btnIngresos"
                app:layout_constraintEnd_toEndOf="@+id/basicos_btnIngresos"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline" app:layout_constraintGuide_begin="174dp"
                                                android:orientation="vertical"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline3"
                                                android:orientation="vertical" app:layout_constraintGuide_end="183dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>